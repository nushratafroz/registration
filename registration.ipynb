    "        moving_slice = nifti_volume[:, :, i]\n",
    "        \n",
    "        # Save the moving slice as an individual NIfTI image before registration\n",
    "        moving_slice_output_path = os.path.join(individual_slices_dir, f'moving_slice_{i}.nii')\n",
    "        sitk.WriteImage(moving_slice, moving_slice_output_path)\n",
    "        \n",
    "        # Generate unique file names for each pair of slices\n",
    "        out_img_file = os.path.join(individual_slices_dir, f'registered_image_{i}.nii')\n",
    "        \n",
    "        # Call the registration function\n",
    "        registered_slice = do(fixed_slice, moving_slice, out_img_file)\n",
    "        \n",
    "        # Call the registration function to perform the registration and save it\n",
    "        do(fixed_slice, moving_slice, out_img_file)\n",
    "\n",
    "# Save the entire 3D volume as a NIfTI file\n",
    "registered_volume_output_path = os.path.join(out_img_dir, 'registered_volume_1_cine.nii')\n",
    "sitk.WriteImage(nifti_volume, registered_volume_output_path)\n",
    "\n"
   ]
