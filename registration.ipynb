# Specify the folder containing JSON files
json_folder_path = r"D:\CSS\cineshiga\cineMRI20231102\1cine\1cine"

# Specify the path to your 4D image data
img_data_path = r'D:\CSS\CSS 1\1-cine.img'

# Load the 4D image data
img_all = nib.load(img_data_path)

# Get the NIfTI header
nifti_header = img_all.header

# Extract voxel sizes from NIfTI header
voxel_sizes = nifti_header["pixdim"][1:4]

# Get the dimensions of the 4D image
x_dim, y_dim, z_dim, t_dim = img_all.shape

# Initialize a list to store MR signals for each control point
mr_signals_list = []

# Initialize a counter for file number
file_number = 1

# Iterate over each JSON file in the folder
for json_file_name in os.listdir(json_folder_path):
    if json_file_name.endswith(".json"):
        # Reset slice_index for each file
        slice_index = 0
        
        json_file_path = os.path.join(json_folder_path, json_file_name)

        # Open the JSON file and load its contents
        with open(json_file_path, 'r') as json_file:
            json_data = json.load(json_file)

        # Extract control points from the JSON file
        point_list = json_data['markups'][0]['controlPoints']

        # Initialize an array to store MR signals for the current control point
        mr_signals_array = np.zeros((len(point_list), img_all.shape[-1]))

        # Iterate over each slice
        for slice_index in range(img_all.shape[-1]):
            # Iterate over each control point
            for pt_no, pt in enumerate(point_list):
                # Extract coordinates from JSON
                x_mm, y_mm, z_mm = map(float, pt['position'])
                print(f"Point: {file_number}, Coordinates from JSON - x: {x_mm}, y: {y_mm}, z: {z_mm}")

                # Convert spatial coordinates from millimeters to voxel indices
                x_voxel = int(x_mm / voxel_sizes[0])
                y_voxel = int(y_mm / voxel_sizes[1])
                z_voxel = int(z_mm / voxel_sizes[2])

                # Retrieve the MR signal at the specified voxel coordinates for the current slice
                mr_signal = img_all.get_fdata()[x_voxel, y_voxel, z_voxel, slice_index]

                # Store the MR signal for the current control point in the array
                mr_signals_array[pt_no][slice_index] = mr_signal

                # Print the MR signal for the current control point, slice, and frame
                print(f"MR Signal for {json_file_name}: {mr_signal}")

            # Increment the file number
            file_number += 1
            # Increment the slice index for the next iteration
            slice_index += 1

            #pt_no=pt_no+1
