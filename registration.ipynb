    "folder_path = r\"D:\\CSS\\cineshiga\\cineMRI20231102\\1cine\\1cine\"\n",
    "\n",
    "# Initialize a list to store control points from all files\n",
    "all_control_points = []\n",
    "\n",
    "# Iterate over all files in the folder\n",
    "for filename in os.listdir(folder_path):\n",
    "    if filename.endswith(\".mrk.json\"):  # Ensure it's a JSON file\n",
    "        file_path = os.path.join(folder_path, filename)\n",
    "\n",
    "        # Open the JSON file and load its contents\n",
    "        with open(file_path, 'r') as json_file:\n",
    "            json_data = json.load(json_file)\n",
    "\n",
    "        # Extract control points from the loaded JSON data\n",
    "        control_points = json_data.get('markups', [])\n",
    "        all_control_points.extend(control_points)\n",
    "\n",
    "\n",
    "# Print the first few slices of the image data\n",
